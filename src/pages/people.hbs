<!DOCTYPE html>
<html>
    <head>
        {{> _head }}
    </head>
    <body>
        <div class="wrapper">
            {{> _sidebar src="img/logo.svg" srcxp="img/logo-expanded.svg" }}
            <div id="content">
                <div class="navbar-header">
                    {{> _navbar-5 src="img/hbg.svg" }}
                </div>
                <div class="container-fluid p-4">
                    <div class="row">
                        <div class="col-lg-3">
                            {{> _card-3 bg="img/p7.jpg" user="img/p1.jpg" }}
                        </div>
                        <div class="col-lg-3">
                            {{> _card-3 bg="img/bg2.png" user="img/p2.jpg" }}
                        </div>
                        <div class="col-lg-3">
                            {{> _card-3 bg="img/bg3.png" user="img/p3.jpg" }}
                        </div>
                        <div class="col-lg-3">
                            {{> _card-3 bg="img/bg4.png" user="img/p4.jpg" }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-3">
                            {{> _card-1 bg="img/bg5.png" user="img/p5.jpg" }}
                        </div>
                        <div class="col-lg-3">
                            {{> _card-1 bg="img/bg6.jpg" user="img/p6.jpg" }}
                        </div>
                        <div class="col-lg-3">
                            {{> _card-1 bg="img/bg7.png" user="img/p7.jpg" }}
                        </div>
                        <div class="col-lg-3">
